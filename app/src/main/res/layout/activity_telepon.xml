<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.agrendra.lat_manifest.TeleponActivity">

    <ImageView
        android:id="@+id/logotlp"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/if_viber_328079"
        android:layout_centerHorizontal="true"/>

    <EditText
        android:id="@+id/txt"
        android:layout_below="@+id/logotlp"
        android:layout_marginTop="20dp"
        android:layout_width="330dp"
        android:layout_height="wrap_content"
        android:hint="Masukan Nomor"
        android:inputType="phone"/>

    <ImageView
        android:id="@+id/logocon"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="25dp"
        android:src="@drawable/logocantact"
        android:layout_toRightOf="@+id/txt"
        android:layout_below="@+id/logotlp"/>

    <Button
        android:id="@+id/btn"
        android:layout_below="@+id/txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/txt"
        android:layout_marginRight="15dp"
        android:text="Proses"
        android:onClick="telp"/>

</RelativeLayout>
